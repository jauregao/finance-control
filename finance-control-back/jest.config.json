{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "src",
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "collectCoverageFrom": [
    "**/*.ts",
    "!**/*.spec.ts",
    "!**/node_modules/**"
  ],
  "coverageDirectory": "../coverage",
  "testEnvironment": "node"
}