{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "src",
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "collectCoverageFrom": [
    "**/*.ts",
    "!**/*.spec.ts",
    "!**/node_modules/**"
  ],
  "coverageDirectory": "../coverage",
  "testEnvironment": "node",
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/$1",
    "^generated/(.*)$": "<rootDir>/../generated/$1"
  },
  "setupFiles": ["<rootDir>/../test/jest.setup.ts"]
}